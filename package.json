{
  "name": "amica-website",
  "version": "0.0.1",
  "description": "Amica's Website",
  "main": "index.js",
  "scripts": {
    "client": "npm run clean && npm run webpack-dev",
    "server": "nodemon server.js",
    "webpack-dev": "webpack --config webpack/webpack.dev.js && webpack-dev-server --config webpack/webpack.dev.js --open",
    "prod": "npm run clean && npm run webpack-prod && node server.js",
    "webpack-prod": "webpack -p --config webpack/webpack.prod.js",
    "lint": "tslint --project . --fix",
    "clean": "rm -rf dist index.html",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "proxy": "http://localhost:5000",
  "author": "Tim Stoddard, James Van, John Cunningham",
  "repository": {
    "url": "https://github.com/timstoddard/amica-website"
  },
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.4.5",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mongoose": "^5.4.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "validator": "^10.11.0",
    "webpack": "^4.28.4",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-loadable": "^5.4.3",
    "@types/react-redux": "^7.0.0",
    "@types/react-router-dom": "^4.3.1",
